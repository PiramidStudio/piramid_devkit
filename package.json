{
  "config": {
    "js": {
      "folder": "./assets/js",
      "src": "./assets/js/**/*.js",
      "dist": "./production/js"
    },
    "stylus": {
      "folder": "./assets/styl",
      "src": "./assets/styl/**/*.styl",
      "dist": "./production/css",
      "style": "compressed"
    },
    "pug": {
      "folder": "./views",
      "src": "./views/**/!(_)*.{pug,jade}",
      "dist": "./production",
      "watch": "./views/**/!(_)*.{pug,jade}"
    },
    "web": {
      "server": "./production",
      "port": "5858",
      "open": false,
      "watchOptions": {
        "ignoreInitial": true,
        "ignored": "*.map"
      }
    },
    "watch": {
      "reload": "./**/*.{html,php,jpg,png,gif,json,js}"
    },
    "jshintConfig": {}
  },
  "devDependencies": {
    "bower": "latest",
    "browser-sync": "^2.10.0",
    "custom-gulp-sass-inheritance": "latest",
    "emitty": "^1.3.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.2.0",
    "gulp-filter": "^3.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imports": "0.0.3",
    "gulp-js-inheritance": "latest",
    "gulp-jshint": "^1.12.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-pug": "latest",
    "gulp-pug-inheritance": "^0.2.1",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-stylus": "latest",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "gutil": "^1.6.4",
    "jeet": "^6.1.2",
    "js-graph-imports": "latest",
    "jshint-stylish": "^2.0.1",
    "page": "latest",
    "path": "^0.12.7",
    "pug-bootstrap": "latest",
    "through2": "^2.0.0",
    "express": "^4.14.0",
    "gulp-changed": "^1.3.2",
    "gulp-filter": "^3.0.1",
    "gulp-flatten": "^0.3.1",
    "gulp-jade-find-affected": "^0.2.2",
    "gulp-modified": "^0.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-requirejs": "^0.4.0",
    "gulp-rsync": "0.0.7",
    "kouto-swiss": "^0.14.0",
    "main-bower-files": "^2.13.1",
    "requirejs": "^2.3.2",
    "rupture": "^0.6.2",
    "semver": "^5.3.0",
    "stylus": "latest",
    "stylus-type-utils": "0.0.3"
  }
}
